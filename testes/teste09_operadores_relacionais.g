
/* Teste 09: Operadores relacionais */
programa {
    int a, b;
    a = 10;
    b = 5;
    
    se (a == 10) entao escreva "a eh 10";
    novalinha;
    
    se (a != b) entao escreva "a diferente de b";
    novalinha;
    
    se (a > b) entao escreva "a maior que b";
    novalinha;
    
    se (b < a) entao escreva "b menor que a";
    novalinha;
}
